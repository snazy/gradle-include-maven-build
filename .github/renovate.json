{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchManagers": ["maven", "gradle", "gradle-wrapper", "npm", "pip_requirements", "pip_setup"],
      "automerge": true,
      "platformAutomerge": true
    }
  ],
  "rebaseWhen": "conflicted",
  "prConcurrentLimit": 50,
  "prHourlyLimit": 10
}
